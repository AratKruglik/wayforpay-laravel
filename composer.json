{
    "name": "aratkruglik/wayforpay-laravel",
    "description": "Native Laravel integration for WayForPay payment gateway.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AratKruglik",
            "email": "oleksii.kruhlyk@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-mutate": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "AratKruglik\\WayForPay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AratKruglik\\WayForPay\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AratKruglik\\WayForPay\\WayForPayServiceProvider"
            ],
            "aliases": {
                "WayForPay": "AratKruglik\\WayForPay\\Facades\\WayForPay"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
